<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="8dp"
    android:background="@drawable/background_border"
    tools:context=".resume"><![CDATA[>













    ]]>

    <TextView
        android:id="@+id/hobbies"
        android:layout_width="363dp"
        android:layout_height="25dp"
        android:layout_marginTop="32dp"
        android:text="Hobbies:"
        app:layout_constraintEnd_toEndOf="@+id/Langues"
        app:layout_constraintStart_toStartOf="@+id/Langues"
        app:layout_constraintTop_toBottomOf="@+id/Langues" />

    <TextView
        android:id="@+id/Langues"
        android:layout_width="363dp"
        android:layout_height="25dp"
        android:layout_marginTop="32dp"
        android:text="Languages:"
        app:layout_constraintEnd_toEndOf="@+id/flutterSkill"
        app:layout_constraintStart_toStartOf="@+id/flutterSkill"
        app:layout_constraintTop_toBottomOf="@+id/flutterSkill" />

    <TextView
        android:id="@+id/iosSkill"
        android:layout_width="363dp"
        android:layout_height="25dp"
        android:layout_marginTop="32dp"
        android:text="IosSkill:"
        app:layout_constraintEnd_toEndOf="@+id/androidSkill"
        app:layout_constraintStart_toStartOf="@+id/androidSkill"
        app:layout_constraintTop_toBottomOf="@+id/androidSkill" />

    <TextView
        android:id="@+id/flutterSkill"
        android:layout_width="363dp"
        android:layout_height="25dp"
        android:layout_marginTop="32dp"
        android:text="FlutterSkill:"
        app:layout_constraintEnd_toEndOf="@+id/iosSkill"
        app:layout_constraintStart_toStartOf="@+id/iosSkill"
        app:layout_constraintTop_toBottomOf="@+id/iosSkill" />

    <TextView
        android:id="@+id/androidSkill"
        android:layout_width="363dp"
        android:layout_height="25dp"
        android:layout_marginTop="32dp"
        android:text="AndroidSkill:"
        app:layout_constraintEnd_toEndOf="@+id/gender"
        app:layout_constraintStart_toStartOf="@+id/gender"
        app:layout_constraintTop_toBottomOf="@+id/gender" />

    <TextView
        android:id="@+id/name"
        android:layout_width="0dp"
        android:layout_height="25dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:text="Name:"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/gender"
        android:layout_width="363dp"
        android:layout_height="25dp"
        android:layout_marginTop="32dp"
        android:text="Gender:"
        app:layout_constraintEnd_toEndOf="@+id/age"
        app:layout_constraintStart_toStartOf="@+id/age"
        app:layout_constraintTop_toBottomOf="@+id/age" />

    <TextView
        android:id="@+id/age"
        android:layout_width="363dp"
        android:layout_height="25dp"
        android:layout_marginTop="32dp"
        android:text="Age"
        app:layout_constraintEnd_toEndOf="@+id/mail"
        app:layout_constraintStart_toStartOf="@+id/mail"
        app:layout_constraintTop_toBottomOf="@+id/mail" />

    <TextView
        android:id="@+id/mail"
        android:layout_width="363dp"
        android:layout_height="25dp"
        android:layout_marginTop="32dp"
        android:text="Email:"
        app:layout_constraintStart_toStartOf="@+id/name"
        app:layout_constraintTop_toBottomOf="@+id/name" />

</androidx.constraintlayout.widget.ConstraintLayout>